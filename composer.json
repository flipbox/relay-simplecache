{
  "name": "flipboxdigital/relay-simplecache",
  "description": "PSR16 - Simple Cache middleware for Relay",
  "version": "2.0.0",
  "keywords": [
    "cache",
    "psr16",
    "psr-16",
    "relay"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "flipboxdigital/relay": "dev-feature/php8",
    "flipboxdigital/http-stream": "dev-feature/php8",
    "psr/simple-cache": "^1|^2|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Relay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Relay\\Tests\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}